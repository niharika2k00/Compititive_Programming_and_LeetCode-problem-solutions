#include <bits/stdc++.h>
#include <iostream>

using namespace std;

#define ll long long
#define vi vector<int>
#define vll vector<ll>
#define pb push_back
#define mp make_pair
#define loop1(n) for (ll i = 0; i < (n); i++)
#define loop2(n) for (ll i = 1; i <= (n); i++)
#define test  \
    ll t;     \
    cin >> t; \
    while (t--)

ll gcd(int a, int b)
{
    if (a == 0)
        return b;
    return gcd(b % a, a);
}

// gcd(a,b) === 1 means ----->   No common factor between the two so prime numbers using SIEVE OF ERATHOSTHENES
vector<int> sievePrime(int x)
{
    vector<int> res;
    for (int i = 2; i * i <= x; i++)
    {
        while (x % i == 0)
        {
            x /= i;
            res.push_back(i);
        }
    }

    if (x != 1)
        res.push_back(x);
    return res;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    ll n, m, element, i, k, ans, count = 0;
    vector<bool> vec(100005, true);
    vector<int> res;
    cin >> n >> m;

    for (i = 0; i < n; i++)
    {
        cin >> element;
        vector<int> v = sievePrime(element);

        for (auto &item : v)
        {
            if (vec[item])
            {
                for (int j = item; j < 100005; j += item)
                    vec[j] = false;
            }
        }
    }

    for (i = 1; i <= m; i++)
        if (vec[i]) // true
            res.push_back(i);

    cout << res.size() << '\n';

    for (auto &nx : res)
        cout << nx << '\n';

    return 0;
}

/* 

INPUT :
3 12
6 1 5

OUTPUT : 
3
1
7
11

 */